{
    "name": "tim/vhosting",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/",
            "SymfonyStandard\\": "app/",
            "App\\": "app"
        },
        "files": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "3.4.*",
        "symfony/swiftmailer-bundle": "@stable",
        "symfony/monolog-bundle": "@stable",

        "twig/twig": "@stable",

        "doctrine/orm": "@stable",
        "doctrine/dbal": "@stable",
        "doctrine/doctrine-bundle": "@stable",
        "doctrine/doctrine-fixtures-bundle": "@stable",

        "sensio/distribution-bundle": "@stable",
        "sensio/framework-extra-bundle": "@stable",
        "sensio/generator-bundle": "@stable",

        "jms/serializer-bundle": "@stable",
        "friendsofsymfony/rest-bundle": "@stable",
        "friendsofsymfony/user-bundle": "@stable",
        "incenteev/composer-parameter-handler": "@stable",
        "liip/functional-test-bundle": "@stable",
        "escapestudios/wsse-authentication-bundle": "@stable",
        "suncat/mobile-detect-bundle": "@stable",
        "tfox/mpdf-port-bundle": "@stable",
        "hwi/oauth-bundle": "@stable",
        "cboden/ratchet": "@stable",
        "gregwar/captcha-bundle": "@stable",

        "sonata-project/admin-bundle": "@stable",
        "sonata-project/block-bundle": "@stable",
        "sonata-project/user-bundle": "@stable",
        "sonata-project/core-bundle": "@stable",
        "sonata-project/datagrid-bundle": "@stable",
        "sonata-project/formatter-bundle": "@stable",
        "sonata-project/doctrine-orm-admin-bundle": "@stable",

        "stof/doctrine-extensions-bundle": "@stable",

        "dropbox/dropbox-sdk": "@stable",
        "google/apiclient": "@stable",
        "dizda/cloud-backup-bundle": "@stable",

        "knplabs/knp-menu-bundle": "@stable",
        "knplabs/knp-paginator-bundle": "@stable",
        "nelmio/api-doc-bundle": "@stable",
        "beberlei/DoctrineExtensions": "@stable",
        "php-http/httplug-bundle": "^1.12",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4",
        "doctrine/doctrine-migrations-bundle": "@stable"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "2.*",
        "pdepend/pdepend": "2.*",
        "phploc/phploc": "2.*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "0.*"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
