{% extends '@TimVhosting/layout.html.twig' %}

 {% block stylesheets %}
     {% stylesheets '@TimVhostingBundle/Resources/public/css/frontend1.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
     {% endstylesheets %}
 {% endblock stylesheets %}


{% block header %}
{% endblock header %}

{% block body %}
<div class="body">
    <div class="wrapper">
        <div class="container sidebar-color">
            <div class="content white" id="content">
                {% block seotext %}
                    <div class="seotext" id="seotext">
                        <p>{{ 'seo_text'|trans({}, 'TimVhostingBundle') }}</p>
                    </div>
                {% endblock seotext %}

                <div class="content-main">
                    <div class="tags-filter">
                        <div class="dropdown-filters">
                            <div class="dropdown">
                                <a class="link dropdown-toggle" href="#" data-toggle="dropdown">
                                    <span class="dropdown-u">{{ 'frontend.tag.best'|trans({}, 'TimVhostingBundle') }}</span>
                                </a>
                                <ul class="dropdown-menu">
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.popular'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link menu-link-active" href="#">{{ 'frontend.tag.best'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.comment'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.new'|trans({}, 'TimVhostingBundle') }}</a></li>
                                </ul>
                            </div>

                            <div class="dropdown">
                                <a class="link dropdown-toggle" href="#" data-toggle="dropdown">
                                    <span class="dropdown-u">{{ 'frontend.tag.for_week'|trans({}, 'TimVhostingBundle') }}</span>
                                </a>
                                <ul class="dropdown-menu">
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.for_all_time'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.today'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link menu-link-active" href="#">{{ 'frontend.tag.week'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.month'|trans({}, 'TimVhostingBundle') }}</a></li>
                                </ul>
                            </div>

                            <div class="dropdown">
                                <a class="link dropdown-toggle" href="#" data-toggle="dropdown">
                                    <span class="dropdown-u">{{ 'frontend.tag.time.any'|trans({}, 'TimVhostingBundle') }}</span>
                                </a>
                                <ul class="dropdown-menu">
                                    <li class="dropdown-item"><a class="menu-link menu-link-active" href="#">{{ 'frontend.tag.time.any'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.time.small'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.time.middle'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.time.long'|trans({}, 'TimVhostingBundle') }}</a></li>
                                    <li class="dropdown-item"><a class="menu-link" href="#">{{ 'frontend.tag.time.verylong'|trans({}, 'TimVhostingBundle') }}</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <nav class="tags collapsed">
                        <a href="#" class="link tag-link pinned weight-2" title="Test1">Test1</a>
                        <a href="#" class="link tag-link pinned weight-1" title="Test2">Test2</a>
                    </nav>
                </div>
            </div>
        </div>

        <aside class="menu">
            <div class="">
                <p class="logo"></p>
                <form method="post" class="search-form">
                    <input class="form-control" name="search" placeholder="{{ 'header.search'|trans({}, 'TimVhostingBundle') }}..."
                           type="text">
                    <input class="search-submit" value="" type="submit">
                </form>
            </div>
            <div class="">
                <div>
                    <a class="menu-link" href="#">Test1</a>
                    <a class="menu-link" href="#">Test2</a>
                    <a class="menu-link" href="#">Test3</a>
                </div>

                <div>
                    <a class="menu-link" href="#">Test4</a>
                    <a class="menu-link" href="#">Test5</a>
                    <a class="menu-link" href="#">Test6</a>
                </div>
            </div>
        </aside>
    </div>
</div>
{% endblock body %}

{% block footer_container %}
{% endblock footer_container %}


{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    {# <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.js"></script> #}
{% endblock javascripts %}